{
    "env": {
      "es2021": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "off", 
      "eqeqeq": ["error", "always"], 
      "curly": ["error", "all"], 
      "prefer-const": "error" 
    }
  }